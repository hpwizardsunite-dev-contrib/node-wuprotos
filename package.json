{
  "name": "node-wuprotos",
  "version": "1.0.0",
  "description": "Harry Potter Wizards Protobuf files from WUProtos, wrapped up in a Node module",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hpwizardsunite-dev-contrib/node-wuprotos.git"
  },
  "keywords": [
    "unite",
    "wizards",
    "hp",
    "api",
    "protobuf",
    "niantic"
  ],
  "author": "hpwizardsunite-dev-contrib",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hpwizardsunite-dev-contrib/node-wuprotos/issues"
  },
  "homepage": "https://github.com/hpwizardsunite-dev-contrib/node-wuprotos#readme",
  "dependencies": {
    "pbjs": "0.0.5",
    "protobufjs": "^6.8.8",
    "run-s": "0.0.0",
    "uglifyjs": "^2.4.11"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "uglify-js": "^3.5.9"
  },
  "typings": "./wuprotos.d.ts",
  "scripts": {
    "test": "node test/test.js",
    "build": "run-s compile minjs uglify tempjs ts",
    "compile": "node proto/compile.js",
    "minjs": "pbjs --keep-case -t static-module --no-verify --no-create --no-delimited --no-beautify --no-comments -o compiled.js ./proto/WUProtos.proto",
    "uglify": "uglifyjs compiled.js -o compiled.js",
    "tempjs": "pbjs --keep-case -t static-module --no-verify --no-create --no-delimited -o temp-ts.js ./proto/WUProtos.proto",
    "ts": "pbts --no-comments -o compiled.d.ts temp-ts.js"
  }
}
